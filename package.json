{
  "name": "ebird-mydata-reader",
  "version": "1.0.0",
  "description": "A library to read eBird's \"My Data\" ZIP file format and convert the data inside to JSON for use in web applications",
  "main": "dist/cjs/ebird-mydata-reader.js",
  "scripts": {
    "watch": "npx webpack --mode development --watch",
    "build": "npx tsc --project tsconfig.esm.json",
    "buildNode": "npx tsc --project tsconfig.commonjs.json",
    "testNode": "node dist/cjs/test-node",
    "buildGlobal": "npx webpack --mode production"
  },
  "author": "dave.rodriguez (ohiodave@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "jszip": "^3.10.1",
    "papaparse": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "jsdoc-to-markdown": "^7.1.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
